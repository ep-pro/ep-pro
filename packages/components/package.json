{
  "name": "@ep-pro/components",
  "type": "module",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      },
      "./resolver": {
        "import": {
          "types": "./dist/resolver.d.ts",
          "default": "./dist/resolver.js"
        },
        "require": {
          "types": "./dist/resolver.d.cts",
          "default": "./dist/resolver.cjs"
        }
      },
      "./imports": {
        "import": {
          "types": "./dist/imports.d.ts",
          "default": "./dist/imports.js"
        },
        "require": {
          "types": "./dist/imports.d.cts",
          "default": "./dist/imports.cjs"
        }
      }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "access": "public"
  },
  "dependencies": {
    "@ep-pro/form": "workspace:*",
    "@ep-pro/table": "workspace:*",
    "@vueuse/core": "catalog:",
    "element-plus": "catalog:",
    "vue": "catalog:"
  },
  "devDependencies": {
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4"
  }
}
