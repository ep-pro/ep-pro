{
  "prettier.enable": true,
  "eslint.enable": true,
  "eslint.validate": [
    "vue",
    "yaml",
    "html",
    "toml",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "cSpell.allowCompoundWords": true,
  "cSpell.words": ["dimm", "execa", "lcov", "tsup", "unplugin", "vitepress", "vitesse", "vueuse"]
}
